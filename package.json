  "dependencies": {
    "apollo-server-micro": "2.24.0",
    "axios": "0.21.4",
    "bluebird": "3.7.2",
    "cep-promise": "4.1.1",
    "cors": "2.8.5",
    "graphql": "15.6.0",
    "lodash": "4.17.21",
    "micro-cors": "0.1.1",
    "mobx": "6.3.3",
    "next": "11.1.2",
    "next-connect": "0.10.2",
    "next-sitemap": "1.6.168",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.2.0",
    "redoc": "2.0.0-rc.53",
    "sass": "1.42.1",
    "selic": "1.0.0",
    "styled-components": "5.3.1",
    "wikijs": "6.3.3"
  },
  "devDependencies": {
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "github-contributors-list": "1.2.5",
    "husky": "4.2.5",
    "jest": "27.2.1",
    "lint-staged": "11.1.2",
    "prettier": "2.4.1",
    "wait-on": "6.0.0"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "fix": "eslint . --ext .js --fix",
    "commit": "cz",
