  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BrasilAPI/BrasilAPI/issues"
  },
  "homepage": "https://github.com/BrasilAPI/BrasilAPI#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
