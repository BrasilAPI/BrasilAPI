                                "schema": {
                                    "$ref": "#/components/schemas/ErrorMessage"
                                },
                                "example": {
                                    "name": "NotFoundError",
                                    "message": "UF não encontrado",
                                    "type": "not_found"
                                }
                            }
                        }
                    }
                }
